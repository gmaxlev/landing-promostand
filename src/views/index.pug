        // ===============================
        // ОКНО "УЗНАТЬ СТОИМОСТЬ РЕКЛАМЫ"
        // ===============================
        .modal#modal-price
            .modal__dark
            .modal__window
                .modal__close
                h2.section-title Узнать стоимость рекламы
                form(action="")#form-price.modal-form
                    .input-text
                        input(type="text", name="name" placeholder="Ваше имя") 
                    .input-text
                        input(type="text", name="phone" placeholder="Ваш телефон").phone-input
                    button(type="submit").button.button_b Отправить
                    .formcomplete.formcomplete_ok 
                        span Спасибо! <br>Мы вам перезвоним.
                    .formcomplete.formcomplete_error
                        span Ошибка!  <br> Не удалось отправить.

        // ===============================
        // ОКНО "УЗНАТЬ СТОИМОСТЬ РЕКЛАМЫ"
        // ===============================
        .modal#modal-question
            .modal__dark
            .modal__window
                .modal__close
                h2.section-title Узнать стоимость рекламы
                form(action="")#form-question.modal-form
                    .input-text
                        input(type="text", name="name" placeholder="Ваше имя") 
                    .input-text
                        input(type="text", name="phone" placeholder="Ваш телефон").phone-input
                    .textarea
                        textarea(name="message", cols="30", rows="10")
                    button(type="submit").button.button_b Отправить
                    .formcomplete.formcomplete_ok 
                        span Спасибо! <br>Мы вам перезвоним.
                    .formcomplete.formcomplete_error
                        span Ошибка!  <br> Не удалось отправить.


